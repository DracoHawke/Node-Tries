<% include header %>
<link rel="stylesheet" type="text/css" href="/assets/css/style.css">
<link rel="stylesheet" type="text/css" href="/assets/css/signup.css">
<link rel="stylesheet" type="text/css" href="/assets/css/dashboard.css">
<script src="/assets/javascript/Files/dashboard.js"></script>
<title>DogMate</title>
</head>
<body>
  <% include navigation %>
  <div class="container-fluid pricing">
    <div class="container">
    <% if(send_data.status_err=='not ver') { %><% console.log("Not verified"); %>
          <h2 class="red">Please Verify Your email to Access This Feature......</h2>
    <%  } else { %><% console.log("verified"); %>
      <div class="row">
        <div class="col-lg-3 menu-card">
          <div class="row">
            <div class="col-12 menu myacc">
              <a class="for_ajax myacc" href="/dashboard/myacc" active><i class='fas fa-user-alt'></i>&nbsp;&nbsp;My Account</a>
            </div>
            <% if(did == 1) { %><% console.log("Have dogs"); %>
              <div class = "col-12 menu myacc">
                <a class="for_ajax mydogs" href="/dashboard/mydogs"><i class="fas fa-dog"></i>&nbsp;&nbsp;My Dog(s)</a>
              </div>
            <% } %>
            <div class="col-12 menu chat">
              <a class="for_ajax chat" href="/dashboard/chat"><i class='fas fa-comment-alt'></i>&nbsp;&nbsp;Chat</a>
            </div>
            <div class="col-12 menu settings">
              <a class="for_ajax settings" href="/dashboard/settings"><i class='fas fa-user-cog'></i>&nbsp;&nbsp;Settings</a>
            </div>
          </div>
        </div>
        <% if(check == "myacc"){ %> <% include myaccount %> <% console.log("myaccount");  %>
        <% } else if(check == "mydogs"){ %> <% include mydogs %> <% console.log("mydogs");  %>
        <% } else if(check == "settings"){ %> <% include settings %> <% console.log("settings");  %>
        <% } else if(check == "dogdetails"){ %> <% include dogdetails %> <% console.log("dogdetails");%>
        <% } else { %> <%= check %> HELLO <% include myaccount %> <% console.log("myaccount default"); } %>
      </div>
      <% } %>
    </div>
  </div>
<% include footer %>
</body>
</html>
