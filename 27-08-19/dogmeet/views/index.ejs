              </ul>
              <a class="ml-4 btn custom-btn" href="/findsitter" >FIND A SITTER</a>
            </div>
          </div>
          <div class="row">
            <div class="line">
            </div>
          </div>
          <div class="row mb-5">
            <div class="col-sm-2 col-md-2 col-lg-2 text-center">
              <h2 class="red">Top</h2><h2 class="red">Mates</h2>
              <a class="text-dark size-1"  href="#car" data-slide="prev"><i class='fas fa-arrow-left'></i></a>
              <a class="text-dark size-1"  href="#car" data-slide="next"><i class='fas fa-arrow-right'></i></a>
            </div>
            <div class="col-sm-10 col-md-10 col-lg-10 owl-carousel owl-theme">
            <% for(var m=0;m<2;m++){ %>
							<% for(var i=m*4;i<(m+1)*4 && i<rows.length ;i++){ %>
							<div class="item" style="">
								<a href="/matedetails?id=<%= rows[i].Email %>&did=<%= rows[i].Did %>">
									<img class="profile" src="<% if(rows[i].DogPic1){%>/<%= rows[i].DogPic1 %><% }else if(rows[i].DogPic2){ %>/<%= rows[i].DogPic2 %><%}else if(rows[i].DogPic3){%>/<%= rows[i].DogPic3%><%}else if(rows[i].DogPic4){%>/<%= rows[i].DogPic4%><%}else if(rows[i].DogPic5){%>/<%= rows[i].DogPic5%><%}else{%>/users/default.png<%}%>" style="width:100%;height: 10em;object-fit: fill;box-shadow:none;">
								</a>
								<div class="fname">
									<span>
									<%= rows[i].DogName %>,
									<% if(rows[i].DogAge >= 1 && rows[i].DogAge < 2) { %>
										<%= rows[i].DogAge %> Yr
									<% } else if(rows[i].DogAge >= 2) { %>
										<%= rows[i].DogAge %> Yrs
									<% } else if(rows[i].DogAge < 1) {%>
										<%= (Number(rows[i].DogAge)/0.1) %> months <% } %>
									</span>
								</div>
								<div class="grey2" style="text-transform:capitalize;">
									<% rows[i].DogBreed = rows[i].DogBreed.replace(/_/g,' '); %>
									<%= rows[i].DogBreed %>
								</div>
								<div class="row">
									<div class="rating col-sm-9" style="font-size:x-small">
									<%
									if((rows[i].Rating*2)%2!=0){ rows[i].Rating=rows[i].Rating-1;}
									for(var j=0;j<rows[i].Rating;j++){
									%>
									<span class="fa fa-star checked"></span>
									<% }
									if((rows[i].Rating*2)%2==1){%>
									<i class="fas fa-star-half-alt checked"></i>
									<% j=j+1;}
									for(var k=0;k<5-j;k++){
									%>
									<span class="far fa-star" style="color:#ed2154"></span>
									<% }%>
									</div>
									<div class="review col-sm-3" style="font-size:smaller">
									<i class="fas fa-vote-yea"></i>&nbsp;<%= rows[i].Reviews %>
									</div>
								</div>
							</div>
							<% } %>
					  <% } %>
            </div>
          </div>
        </div>
      </div>
      <script>
        $('document').ready(function(){
