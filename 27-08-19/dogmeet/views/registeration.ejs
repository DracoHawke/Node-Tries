<% include header %>
<link rel="stylesheet" type="text/css" href="assets/style.css">
<link rel="stylesheet" type="text/css" href="assets/signup.css">
<script src="/assets/registersitter.js"></script>

<title>DogMate</title>
</head>
<body style="
    font-family: 'Roboto', sans-serif;
" class="scrollbar scrollbar-primary">
  <% include navigation %>
    <div class="container">
    <div class="row">
      <div class="col-md-8">
      <div class="row">
          <div class="col-sm-12 col-md-12 col-lg-12 size-1 font-weight-bold mt-3 pl-0 mb-2">
            <a href="/" class="red ">Home</a>&nbsp;/&nbsp;Register
          </div>
      </div>
      <div class="row">
          <div class="col pl-0" style="line-height:2;">
      <span class=" registeration-head" style="border-bottom: solid rgb(236, 54, 100) 2px;">Re</span><span class=" registeration-head">gister</span></div></div>
      <form class=" registration-form size-1" name="myform" method="post" action="<%= data.success %> " enctype="multipart/form-data" onsubmit="return validateform()">
        <div class="form-group row contact-form">
          <div class="col-sm-12">
            <strong>Profile Picture<span class="red">*</span></strong>
          </div>
          <label id="label10" for="file_upload" class="col-sm-10 col-form-label"><img class="image_profile" id="imageload" src="assets/images/profile.png"></label>
          <div class="col-sm-2">
            <input hidden style="width:1%; height:1%;" type="file" class="form-control size-1" name="fileUpload" id="file_upload" accept="image/*" value="" onchange="loadFile(event)">
            <span class="red" id="file_err" style="font-size: smaller;"><%= data.file_err %></span>
          </div>
        </div>
        <div class="form-group row contact-form">
          <div class="col-sm-6">
          <label for="fname" class="col-sm-12 col-form-label pl-0 pr-0"><strong>First Name<span class="red">*</span></strong></label>
          <div class="col-sm-12 pl-0 pr-0">
            <input type="text" class="form-control size-1" name="fname" id="name" value="" placeholder="Enter Your First Name">
            <span class="red" id="fname_err" style="font-size: smaller;"><%= data.fname_err %></span>
          </div></div><div class="col-sm-6">
          <label for="lname" class="col-sm-12 col-form-label pl-0 pr-0"><strong>Last Name<span class="red">*</span></strong></label>
            <div class="col-sm-12 pl-0 pr-0">
                <input type="text" class="form-control size-1" name="lname" value="" placeholder="Enter Your Last Name">
                <span class="red" id="lname_err" style="font-size: smaller;"><%= data.lname_err %></span>
            </div>
          </div></div>
          <div class="form-group row contact-form">
            <div class="col-sm-6 ">
            <label for="Email" class="col-sm-12 col-form-label pl-0 pr-0"><strong>Email Address<span class="red">*</span></strong></label>
            <div class="col-sm-12 pl-0 pr-0">
              <input type="text" class="form-control size-1" name="email" id="email" value="" placeholder="Enter Email">
              <span class="red" id="email_err" style="font-size: smaller;"><%= data.email_err %></span>
            </div>
          </div>
          <div class="col-sm-6">
            <label for="Phone" class="col-sm-12 col-form-label pl-0 pr-0"><strong>Phone<span class="red">*</span></strong></label>
            <div class="col-sm-12 pl-0 pr-0">
              <input type="text" class="form-control size-1" name="phone" value="" placeholder="Enter Phone">
              <span class="red" id="phone_err" style="font-size: smaller;"><%= data.phone_err %></span>
            </div>
          </div>
          </div>
          <div class="form-group row contact-form">
            <div class="col-sm-6">
            <label for="inputPassword" class="col-sm-12 col-form-label pl-0 pr-0"><strong>Password<span class="red">*</span></strong></label>
              <input type="password" class="form-control size-1" id="inputPassword" name="password" placeholder="Enter Password">
              <span class="red" id="password_err" style="font-size: smaller;"><%= data.password_err %></span>
            </div>
            <div class="col-sm-6">
            <label for="confirmPassword" class="col-sm-12 pl-0 pr-0 col-form-label"><strong>Confirm Password<span class="red">*</span></strong></label>
            <div class="col-sm-12 pl-0 pr-0">
              <input type="password" name="confirmPass" class="form-control size-1" id="inputPassword" placeholder="Re-enter Password">
              <span class="red" id="confirm_err" style="font-size: smaller;"><%= data.confirm_err %></span>
            </div>
          </div></div>
          <div class="col-sm-12 agree">
            By joining you agree to <a href="/terms">Our Terms and Conditions</a> and <a href="/policy">Privacy Policy</a>
          </div>
          <div class="form-group row contact-form">
            <div class="col-sm-12 pl-0">
              <button type="submit" class="btn custom-btn size-1 mt-4 mb-3" style="font-size:1em;padding-left:0.5rem;padding-right:0.5rem;width:10em">Register Now</button>
            </div>
          </div>
        </form>
    </div>
    <div class="registraion-image col-md-4">
      <img src="/assets/images/img-6842.jpg" style="width:100%;height:100%;" alt="">
    </div>
    </div>
    </div>
  <% include footer %>
</body>
</html>
